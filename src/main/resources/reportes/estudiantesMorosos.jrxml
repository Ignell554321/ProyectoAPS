		<queryString language="SQL">
			<![CDATA[SELECT 
e.dni,
CONCAT(e.apellidos,' ',e.nombres) AS nombres,
c.nombre AS curso,
m.tipo_de_maquina AS maquina,
i.fecha_actual,
i.monto_total,
i.monto_restante
FROM inscripcion i inner join estudiante e on (i.estudiante_dni=e.dni)
							inner join curso c on(i.curso_id=c.id)
                            inner join maquina m on(m.id=i.maquina_id) 
                            WHERE i.monto_restante>0]]>
		</queryString>
